{
  "types": [
    "feat",
    "fix",
    "docs",
    "style",
    "refactor",
    "perf",
    "test",
    "build",
    "ci",
    "chore",
    "revert"
  ],
  "format": "<type>(<scope>): <subject>",
  "maxSubjectLength": 72,
  "minSubjectLength": 3,
  "subjectPattern": ".+",
  "subjectPatternError": "subject must not be empty",
  "scopes": [],
  "allowEmptyScope": true,
  "allowCustomScopes": true
}
